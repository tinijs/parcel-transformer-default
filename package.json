    "@parcel/ts-utils": "^2.9.3",
    "@parcel/types": "^2.9.3",
    "fs-extra": "^11.1.1",
    "minify-html-literals": "^1.3.5",
    "recursive-readdir": "^2.2.3",
    "sass": "^1.64.1"
  },
  "peerDependencies": {
    "typescript": ">=5.1.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
