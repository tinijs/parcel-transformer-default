{
  "name": "@tinijs/parcel-transformer-default",
  "version": "0.1.0",
  "description": "A Parcel transformer for TiniJS apps.",
  "author": "Lam Nhan",
  "homepage": "https://lamnhan.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/index.js",
  "source": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "!**/*.ts",
    "**/*.d.ts"
  ],
  "engines": {
    "node": ">= 12.0.0",
